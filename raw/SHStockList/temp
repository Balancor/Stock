                   return CurrentDate;
}

function isValidStockCode(stockCode) {

    var pattern = /^\d{6}$/;

    if(stockCode.search(pattern) == -1) {

        return false;

    } else {

        return true;

    }

}



function isValidDate(date) {

    

    var pattern = /^(19|20)\d\d[- /.](0[1-9]|1[012])[- /.](0[1-9]|[12][0-9]|3[01])$/;

    if(date.search(pattern) == -1) {

        return false;

    } else {

        return true;

    }

}



function isLeapYear(year) {

    return (year % 100 != 0 && year % 4 == 0) || (year % 400 == 0);

}





function adjustMonthOfDateObject(date, offset) {



    var curYear = date.getFullYear();

    var curMonth = date.getMonth();

    var curDate = date.getDate();



    var m = curMonth + offset;



    var newYear = date.getFullYear() + Math.floor(m / 12);

    

    var newMonth = 0;

    if(m < 0) {

        newMonth = ( m % 12 ) + 12;

    } else {

        newMonth = m % 12;

    }



    var newDate = curDate;



    switch(newMonth) {

        case 3: case 5: case 8: case 10:

            newDate = newDate <= 30 ? newDate : 30;

            break;

        case 1:

            var maxDate = isLeapYear(newYear) ? 29 : 28;

            if(newDate > maxDate) {

                newDate = maxDate;

            }

            break;

    }



    var tDate = new Date();

    tDate.setFullYear(newYear, newMonth, newDate);



    return tDate;



}





function adjustDateOfSpecifiedDate(date, offset) {



    var bDate = new Date();



    bDate.setFullYear(date.getFullYear());

    bDate.setMonth(date.getMonth());

    bDate.setDate(date.getDate() + offset);



    return bDate;



}



function dateToString(date) {



    var s = "";



    var year = date.getFullYear();

    var month = (date.getMonth() + 1);

    var day = date.getDate();

    

    s += year;



    s += "-";

    if(month.toString().length < 2) {

        s = s + "0" + month;

    } else {

        s = s + month;

    }



    s += "-";

    if(day.toString().length < 2) {

        s = s + "0" + day;

    } else {

        s = s + day;

    }



    return s;

}



function queryPeriodNotExceedXMonths(beginDate, endDate, months) {



    var flag = true;



    if(beginDate == null || beginDate == "" || endDate == null || endDate == "") {

        return flag;

    }



    try {



        var d1 = beginDate.split("-");

        var d2 = endDate.split("-");



        var date1 = new Date();

        date1.setFullYear(d1[0]);

        date1.setMonth(d1[1] - 1);

        date1.setDate(d1[2]);



        var date2 = new Date();

        date2.setFullYear(d2[0]);

        date2.setMonth(d2[1] - 1);

        date2.setDate(d2[2]);



        var date3 = adjustMonthOfDateObject(date1, months);



        if(date3 < date2) {

            flag = false;

        }



    } catch(ex) {

    }



    return flag;



}


