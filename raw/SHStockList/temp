                   else
                   {
                    CurrentDate += "0" + Month + "-";
                   }
                   if (Day >= 10 )
                   {
                    CurrentDate += Day ;
                   }
                   else
                   {
                    CurrentDate += "0" + Day ;
                   }
                   return CurrentDate;
                }
