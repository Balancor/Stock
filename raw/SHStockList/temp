var errRpted = false;
function trim(s) {

    return s.replace(/^\s+|\s+$/, '');

}

function checkStockCodeLength(s){
    if (s.length != 6){
     alert("证券代码必须为6位数字");
     errRpted = true;
     return false;
    }
     return true;
}

function getFormatDate()
                {
                   var day = new Date();
                   var Year = 0;
                   var Month = 01;
                   var Day = 01;
                   var CurrentDate = "";
                   Year       = day.getFullYear();
                   CurrentDate = Year + "-"+"0"+Month+"-"+"0"+Day;
                   return CurrentDate;
}



function getNowFormatDateByCN(){
                   var day = new Date();
                   var Year = 0;
                   var Month = 0;
                   var Day = 0;
                   var CurrentDate = "";
                   Year       = day.getFullYear();
                   Month      = day.getMonth()+1;
                   Day        = day.getDate();
                   
                   CurrentDate += Year + "年";
                   
                   if (Month >= 10 )
                   {
                    CurrentDate += Month + "月";
                   }
                   else
                   {
                    CurrentDate += "0" + Month + "月";
                   }
                   if (Day >= 10 )
                   {
                    CurrentDate += Day+"日" ;
                   }
                   else
                   {
                    CurrentDate += "0" + Day+"日" ;
                   }
                   return CurrentDate;
                }

function getBeforeFormatDate()
                {
                   var day = new Date();
                   var Year = 0;
                   var Month = 0;
                   var Day = 0;
                   var CurrentDate = "";
                   Year       = day.getFullYear()-1;
                   Month      = day.getMonth()+1;
                   Day        = day.getDate();
                   
                   CurrentDate += Year + "-";
                   
                   if (Month >= 10 )
                   {
                    CurrentDate += Month + "-";
                   }
                   else
                   {
                    CurrentDate += "0" + Month + "-";
                   }
                   if (Day >= 10 )
                   {
                    CurrentDate += Day ;
                   }
                   else
                   {
                    CurrentDate += "0" + Day ;
                   }
                   return CurrentDate;
}
